  "version": "0.2.0",
  "configurations": [
    {
      "name": "Student Find All",
      "request": "launch",
      "type": "dart",
      "program": "${workspaceFolder}/lab_cli/bin/lab_cli.dart",
      "args": [
        "students",
        "findAll"
      ]
    },
    {
      "name": "Student Find By Id",
      "request": "launch",
      "type": "dart",
      "program": "${workspaceFolder}/lab_cli/bin/lab_cli.dart",
      "args": [
        "students",
        "byId",
        "-i",
        "${input:id_student}"
      ]
    },
    {
      "name": "Student Insert",
      "request": "launch",
      "type": "dart",
      "program": "${workspaceFolder}/lab_cli/bin/lab_cli.dart",
      "args": [
        "students",
        "insert",
        "-f",
        "${input:file_path}"
      ]
    },
  ],
  "inputs": [
    {
      "id": "id_student",
      "description": "Informe o ID do Aluno",
      "default": "",
      "type": "promptString"
    },
    {
      "id": "file_path",
      "description": "Informe o caminho do arquivo",
      "default": "",
      "type": "promptString"
    }
  ]
}